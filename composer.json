{
    "name": "graham-campbell/flysystem",
    "description": "Flysystem Is A Flysystem Bridge For Laravel 4.1/4.2",
    "keywords": ["laravel", "framework", "files", "aws", "s3", "dropbox", "flysystem", "Flysystem", "Laravel Flysystem", "Laravel-Flysystem", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/cache": "~4.1",
        "illuminate/support": "~4.1",
        "graham-campbell/manager": "~1.0",
        "league/flysystem": "~1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0",
        "league/flysystem-aws-s3-v2": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "league/flysystem-copy": "~1.0",
        "league/flysystem-dropbox": "~1.0",
        "league/flysystem-eventable-filesystem": "~1.0",
        "league/flysystem-rackspace": "~1.0",
        "league/flysystem-sftp": "~1.0",
        "league/flysystem-webdav": "~1.0",
        "league/flysystem-ziparchive": "~1.0"
    },
    "suggest": {
        "league/event": "Enables usage of the eventable flysystem.",
        "aws/aws-sdk-php": "Enables the aws connector.",
        "barracuda/copy": "Enables the copy connector.",
        "rackspace/php-opencloud": "Enables the rackspace connector.",
        "dropbox/dropbox-sdk": "Enables the dropbox connector.",
        "sabre/dav": "Enables the webdav connector."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Flysystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Flysystem\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
