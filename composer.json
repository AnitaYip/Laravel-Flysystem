{
    "name": "graham-campbell/flysystem",
    "description": "Flysystem Is A Flysystem Bridge For Laravel 5",
    "keywords": ["laravel", "framework", "files", "aws", "s3", "dropbox", "flysystem", "Flysystem", "Laravel Flysystem", "Laravel-Flysystem", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/testbench"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/cache": "~5.0",
        "illuminate/contracts": "~5.0",
        "illuminate/support": "~5.0",
        "graham-campbell/manager": "~2.0",
        "league/flysystem": "~0.5.10"
    },
    "require-dev": {
        "orchestra/testbench": "dev-fix-logging as 3.0.x-dev",
        "graham-campbell/testbench": "~2.0",
        "league/event": "~1.0",
        "aws/aws-sdk-php": "~2.4",
        "barracuda/copy": "~1.1",
        "dropbox/dropbox-sdk": "~1.1",
        "rackspace/php-opencloud": "~1.8",
        "sabre/dav": "~2.0.2"
    },
    "suggest": {
        "league/event": "Enables usage of the eventable flysystem.",
        "aws/aws-sdk-php": "Enables the aws connector.",
        "barracuda/copy": "Enables the copy connector.",
        "rackspace/php-opencloud": "Enables the rackspace connector.",
        "dropbox/dropbox-sdk": "Enables the dropbox connector.",
        "sabre/dav": "Enables the webdav connector."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Flysystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Flysystem\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
